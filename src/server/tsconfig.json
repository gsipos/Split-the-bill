{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"charset": "utf-8",
		"declaration": false,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"outDir": "../../app/server",
		"rootDirs": [
			"./",
			"../shared"
		],
		"experimentalDecorators": true,
		"noUnusedLocals": true,
		"forceConsistentCasingInFileNames": false
	}
}
