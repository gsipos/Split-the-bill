{
	"compilerOptions": {
		"target": "es2015",
		"module": "es2015",
		"moduleResolution": "node",
		"charset": "utf-8",
		"declaration": false,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"outDir": "../../app/server",
		"experimentalDecorators": true,
		"noUnusedLocals": true,
		"forceConsistentCasingInFileNames": false
	}
}
