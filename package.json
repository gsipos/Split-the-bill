{
  "name": "split-the-bill",
  "version": "1.0.0",
  "description": "",
  "main": "./app/server/server.js",
  "scripts": {
    "clientTsc": "tsc -p src/client",
    "ctsc:w": "tsc -p src/client -w",
    "serverTsc": "tsc -p src/server",
    "tsc": "npm run clientTsc & npm run serverTsc",
    "clean": "del ./lib",
    "test": "echo \"Error: no test specified\" && exit 0",
    "liveLog": "azure site log tail split-the-bill",
    "serve": "live-server --open=app/index.html",
    "postinstall": "typings install && npm run tsc",
    "build-less": "catw -c 'lessc -' 'src/**/*.less' > app/client/app.css",
    "watch-less": "catw -c 'lessc -' 'src/*.less' -o app/client/app.css -v"
  },
  "author": "Gergely Sipos",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/forms": "0.1.1",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router": "3.0.0-alpha.7",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.3",
    "@types/azure": "^0.9.17",
    "@types/express": "^4.0.29",
    "@types/express-serve-static-core": "^4.0.29",
    "@types/jasmine": "^2.2.30",
    "@types/jquery": "^1.10.27",
    "@types/mime": "0.0.28",
    "@types/node": "^4.0.30",
    "@types/node-uuid": "0.0.27",
    "azure": "^0.10.6",
    "core-js": "^2.4.0",
    "express": "4.14.0",
    "highland": "2.8.1",
    "node-uuid": "1.4.7",
    "reflect-metadata": "0.1.3",
    "rxjs": "^5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "catw": "^1.0.1",
    "del-cli": "0.2.0",
    "less": "2.7.1",
    "live-server": "^1.0.0",
    "require-dir": "0.3.0",
    "tslint": "3.11.0",
    "typescript": "2.0.0",
    "typings": "^1.3.0"
  },
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  }
}
