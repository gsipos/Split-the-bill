{
  "name": "split-the-bill",
  "version": "1.0.0",
  "description": "",
  "main": "./app/server/server.js",
  "scripts": {
    "clean:nmp": "npm prune",
    "clean:tmp": "rimraf ./.tmp",
    "clean:appclient": "rimraf ./app/client",
    "clean:appserver": "rimraf ./app/server",
    "clean": "run-p clean:*",
    "tsc:client": "tsc -p src/client",
    "tsc:server": "tsc -p src/server",
    "tsc": "npm-run-all --parallel tsc:*",
    "watch:tsc:client": "tsc -p src/client -w",
    "watch:tsc:server": "tsc -p src/server -w",
    "watch:webpack": "webpack -w",
    "watch": "run-p watch:*",
    "webpack": "webpack --config webpack.config.js --display-error-details",
    "build": "npm-run-all clean tsc webpack",
    "exp": "npm-run-all tsc:client build:webpack",
    "postinstall": "npm run build",
    "html:copy": "copyfiles -u 2 src/client/**/*.html app/client/",
    "test": "echo \"Error: no test specified\" && exit 0",
    "serve": "live-server",
    "serve2": "npm run build & start-local.cmd",
    "liveLog": "azure site log tail split-the-bill"
  },
  "author": "Gergely Sipos",
  "license": "MIT",
  "dependencies": {
    "@types/azure": "0.9.18",
    "@types/body-parser": "0.0.34",
    "@types/compression": "0.0.33",
    "@types/cors": "2.8.0",
    "@types/express": "4.0.35",
    "@types/gapi.auth2": "0.0.34",
    "@types/jasmine": "2.5.43",
    "@types/jquery": "2.0.40",
    "@types/mime": "0.0.29",
    "@types/node": "7.0.5",
    "@types/node-uuid": "0.0.28",
    "@types/react": "^15.0.13",
    "@types/react-dom": "^0.14.23",
    "@types/rx": "4.1.1",
    "@types/whatwg-fetch": "0.0.33",
    "awesome-typescript-loader": "^3.0.8",
    "azure": "1.2.0-preview",
    "body-parser": "^1.16.1",
    "compression": "^1.6.2",
    "core-js": "2.4.1",
    "cors": "^2.8.1",
    "css-loader": "^0.26.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "feathers": "^2.0.3",
    "feathers-authentication": "^1.0.2",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.5.0",
    "feathers-hooks": "^1.7.1",
    "feathers-levelup": "^1.1.1",
    "feathers-rest": "^1.6.0",
    "feathers-socketio": "^1.4.3",
    "highland": "2.10.2",
    "html-webpack-plugin": "^2.28.0",
    "node-uuid": "1.4.7",
    "npm-run-all": "^4.0.2",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rxjs": "5.2.0",
    "typescript": "2.2.1",
    "webpack": "^2.2.1",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "live-server": "1.2.0",
