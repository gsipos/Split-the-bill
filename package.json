{
  "name": "split-the-bill",
  "version": "1.0.0",
  "description": "",
  "main": "./app/server/server.js",
  "scripts": {
    "clean:nmp": "npm prune",
    "clean:tmp": "rimraf ./.tmp",
    "clean:appclient": "rimraf ./app/client",
    "clean:appserver": "rimraf ./app/server",
    "clean": "run-p clean:*",
    "tsc:client": "tsc -p src/client",
    "tsc": "npm-run-all --parallel tsc:*",
    "watch:tsc:client": "tsc -p src/client -w",
    "watch:webpack": "webpack -w",
    "watch": "run-p watch:*",
    "webpack": "webpack --config webpack.config.js --progress --display-error-details",
    "build": "npm-run-all clean tsc webpack",
    "exp": "npm-run-all tsc:client build:webpack",
    "postinstall": "npm run build",
    "html:copy": "copyfiles -u 2 src/client/**/*.html app/client/",
    "test": "echo \"Error: no test specified\" && exit 0",
    "serve": "live-server ./app/client",
    "serve2": "npm run build & start-local.cmd",
    "liveLog": "azure site log tail split-the-bill"
  },
  "author": "Gergely Sipos",
  "license": "MIT",
  "dependencies": {
    "@types/gapi.auth2": "0.0.32",
    "@types/jasmine": "2.5.40",
    "@types/node": "7.0.0",
    "@types/node-uuid": "0.0.28",
    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.26.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "firebase": "^3.7.4",
    "html-webpack-plugin": "^2.28.0",
    "live-server": "^1.2.0",
    "node-uuid": "1.4.7",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.1-smooth",
    "typescript": "2.2.1",
    "webpack": "^2.3.2"
  },
  "devDependencies": {
    "firebase-tools": "^3.5.0",
    "live-server": "^1.0.0",
    "rimraf": "^2.5.4",
    "tslint": "4.1.1"
  },
  "private": true,
  "engines": {
    "node": "5.9.1",
    "npm": "3.9.5"
  }
}
